    "pinia-plugin-persistedstate": "^4.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/vite": "^4.1.6",
