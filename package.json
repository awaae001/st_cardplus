{
  "name": "sillytavern-char_cardplus",
  "author": "awaae001",
  "description": "快速卡片格式化工具",
  "private": true,
  "version": "0.1.3",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/pre-build.js && vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "start": "concurrently -k \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build:electron": "cross-env NODE_ENV=production vite build && tsc -p electron/tsconfig.json && electron-builder",
    "electron:serve": "cross-env NODE_ENV=development vite build && tsc -p electron/tsconfig.json && electron ."
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@tailwindcss/vite": "^4.0.8",
    "element-plus": "^2.9.5",
    "file-saver": "^2.0.5",
    "tailwindcss": "^4.0.8",
    "vue": "^3.5.13",
    "vue-router": "4"
  },
  "devDependencies": {
    "@iconify/vue": "^4.3.0",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^22.13.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "terser": "^5.39.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.3",
    "vite": "^6.2.0",
    "vite-plugin-electron": "^0.29.0",
    "vue-tsc": "^2.2.4",
    "wait-on": "^8.0.2"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "electron",
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "electron",
      "vue-demi"
    ]
  }
}
